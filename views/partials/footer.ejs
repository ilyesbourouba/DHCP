        <script>
            var pageName = "<%- pageName %>";
            console.log("pageName: ", pageName);
            switch (pageName) {
                case "Dashboard":
                    $('ul li .nav-link').removeClass("active");
                    $('#dashboard').addClass("active");
                    break;
                case "Users":
                    $('ul li .nav-link').removeClass("active");
                    $('#users').addClass("active");
                    break;
                case "Brand":
                    $('ul li .nav-link').removeClass("active");
                    $('#brand').addClass("active");
                    break;
                case "C/SKD Suppliers And O/C Brands":
                    $('ul li .nav-link').removeClass("active");
                    $('#supplier').addClass("active");
                    break;
                case "Products And Categories":
                    $('ul li .nav-link').removeClass("active");
                    $('#product_item').addClass("active");
                    break;
                case "Defects":
                    $('ul li .nav-link').removeClass("active");
                    $('#defects').addClass("active");
                    break;
                case "Zones And Workshops":
                    $('ul li .nav-link').removeClass("active");
                    $('#zones').addClass("active");
                    break;
                case "RMA Requests":
                    $('ul li .nav-link').removeClass("active");
                    $('#rma').addClass("active");
                    break;
                case "Invalid RMA Requests":
                    $("ul li .nav-link").removeClass("active");
                    $('#invalid_rma').addClass("active");
                    break;
                default:
                    break;
            }
        </script>


